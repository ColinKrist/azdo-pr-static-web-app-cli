{
    "$schema": "https://json.schemastore.org/vss-extension.json",
    "manifestVersion": 1,
    "id": "vercel-azdo",
    "name": "Vercel AzDo Pipelines Tools",
    "version": "0.0.9",
    "publisher": "colinkrist",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Build and release your application on Vercel using this open source integration using the @vercel/client npm package.",
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Vercel",
        "Continuous Delivery",
        "Continuous Integration",
        "DevOps",
        "Release",
        "Next.js",
        "Now"
    ],
    "icons": {
        "default": "images/icon.png",
        "large": "images/icon.png"
    },
    "files": [
        {
            "path": "Task/Deploy"
        }
    ],
    "contributions": [
        {
            "id": "vercel-azdo-deploy",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Task/Deploy"
            }
        }
    ],
    "screenshots": [
        {
            "path": "images/1_test.png"
        }
    ]
}