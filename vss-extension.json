{
    "$schema": "https://json.schemastore.org/vss-extension.json",
    "manifestVersion": 1,
    "id": "medsol-vercel",
    "name": "MedSol Vercel",
    "version": "0.0.3",
    "publisher": "colinkrist",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "A helpful wrapper for Azure Devops around the Vercel CLI",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "icon.png"
    },
    "files": [
        {
            "path": "Task"
        }
    ],
    "contributions": [
        {
            "id": "medsol-vercel",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Task"
            }
        }
    ]
}