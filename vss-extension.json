{
    "$schema": "https://json.schemastore.org/vss-extension.json",
    "manifestVersion": 1,
    "id": "vercel-azdo",
    "name": "Vercel for Azure DevOps Pipelines",
    "version": "0.4.3",
    "publisher": "colinkrist",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Release web apps on Vercel using this open source integration that uses the @vercel/client npm package.",
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "Vercel",
        "Continuous Delivery",
        "Continuous Integration",
        "DevOps",
        "Release",
        "Next.js",
        "Now"
    ],
    "icons": {
        "default": "images/icon.png",
        "large": "images/icon.png"
    },
    "files": [
        {
            "path": "Task/Deploy"
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/ColinKrist/vercel-azdo"
    },
    "contributions": [
        {
            "id": "vercel-azdo-deploy",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Task/Deploy"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/ColinKrist/vercel-azdo"
    },
    "screenshots": [
        {
            "path": "images/pr-comment.png"
        },
        {
            "path": "images/cli-initial-setup.png"
        },
        {
            "path": "images/create_token.png"
        }
    ]
}